<interface>
  <menu id="menubar">
    <section id="menubar-main-section">
      <submenu id="menubar-file">
        <section id="menubar-file-open-section">
          <submenu id="menubar-file-import">
            <section id="menubar-file-import-section">
              <item>
                <attribute name="label" translatable="yes">Import from Mt940</attribute>
                <attribute name="action">win.file.import.mt940</attribute>
              </item>
              <item>
                <attribute name="label" translatable="yes">Import from Mt942</attribute>
                <attribute name="action">win.file.import.mt942</attribute>
              </item>
              <item>
                <attribute name="label" translatable="yes">Import from DTAUS</attribute>
                <attribute name="action">win.file.import.dtaus</attribute>
              </item>
              <item>
                <attribute name="label" translatable="yes">Import from DTAUS and send...</attribute>
                <attribute name="action">win.file.import.dtaus-send</attribute>
              </item>
              <item>
                <attribute name="label" translatable="yes">Import from CSV (AqB)</attribute>
                <attribute name="action">win.file.import.aqb-csv</attribute>
              </item>
              <item>
                <attribute name="label" translatable="yes">Import from CSV (AqB) and send...</attribute>
                <attribute name="action">win.file.import.aqb-csv-send</attribute>
              </item>
            </section>
          </submenu>
        </section>
      </submenu>
      <submenu id="menubar-actions">
        <submenu id="menubar-actions-online">
          <section id="menubar-actions-online-section">
            <section id="menubar-actions-online-fetch-section">
              <item>
                <attribute name="label" translatable="yes">Fetch Balance</attribute>
                <attribute name="action">win.actions.online.get.balance</attribute>
              </item>
              <item>
                <attribute name="label" translatable="yes">Fetch Transaction</attribute>
                <attribute name="action">win.actions.online.get.transaction</attribute>
              </item>
            </section>
            <section id="menubar-actions-online-issue-section">
              <item>
                <attribute name="label" translatable="yes">Issue Transaction</attribute>
                <attribute name="action">win.actions.online.issue.transaction.normal</attribute>
              </item>
              <item>
                <attribute name="label" translatable="yes">Issue SEPA Transaction</attribute>
                <attribute name="action">win.actions.online.issue.transaction.sepa</attribute>
              </item>
              <item>
                <attribute name="label" translatable="yes">Issue International Transaction</attribute>
                <attribute name="action">win.actions.online.issue.transaction.int</attribute>
              </item>
              <item>
                <attribute name="label" translatable="yes">Issue Direct Debit Action</attribute>
                <attribute name="action">win.actions.online.issue.transaction.direct-debit</attribute>
              </item>
              <!--<item>
                <attribute name="label" translatable="yes">Issue SEPA Direct Debit Action</attribute>
                <attribute name="action">win.actions.online.issue.transaction.sepa-direct-debit</attribute>
              </item>/-->
            </section>
            <section id="menubar-actions-online-log-section">
              <item>
                <attribute name="label" translatable="yes">View Log Window</attribute>
                <attribute name="action">win.actions.online.view-log</attribute>
              </item>
            </section>
          </section>
        </submenu>
      </submenu>
      <submenu id="menubar-tools">
        <section id="menubar-tools-section">
          <item>
            <attribute name="label" translatable="yes">Online Banking Setup...</attribute>
            <attribute name="action">win.tools.ab.setup</attribute>
          </item>
        </section>
      </submenu>
    </section>
  </menu>
</interface>
