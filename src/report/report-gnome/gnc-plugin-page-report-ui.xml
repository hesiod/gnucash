<interface>
  <menu id="menubar">
    <section id="menubar-main-section">
      <submenu id="menubar-file">
        <section id="menubar-file-print-section">
          <submenu id="menubar-file-export">
            <section id="menubar-file-export-section">
              <item>
                <attribute name="label" translatable="yes">Export PDF</attribute>
                <attribute name="action">win.file.export.pdf</attribute>
              </item>
              <item>
                <attribute name="label" translatable="yes">Export Report</attribute>
                <attribute name="action">win.file.export.report</attribute>
              </item>
            </section>
          </submenu>
        </section>
      </submenu>
      <submenu id="menubar-edit">
        <section id="menubar-edit-style-sheets-section">
          <item>
            <attribute name="label" translatable="yes">Report Options</attribute>
            <attribute name="action">win.edit.report-options</attribute>
          </item>
        </section>
      </submenu>
      <submenu id="menubar-reports">
        <section id="menubar-reports-standard-reports-section"></section>
        <section id="menubar-reports-saved-reports-section"></section>
        <section id="menubar-reports-account-reports-section"></section>
        <section id="menubar-reports-other-reports-section">
          <item>
            <attribute name="label" translatable="yes">Save Report</attribute>
            <attribute name="action">win.report.save</attribute>
          </item>
          <item>
            <attribute name="label" translatable="yes">Save Report As</attribute>
            <attribute name="action">win.report.save-as</attribute>
          </item>
        </section>
      </submenu>
    </section>
  </menu>
</interface>

